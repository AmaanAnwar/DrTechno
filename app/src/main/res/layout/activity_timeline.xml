<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent"    tools:context=".timeline">   <ImageView       android:layout_width="180dp"       android:layout_height="100dp"       android:src="@drawable/logo"       android:id="@+id/logo"       android:layout_centerHorizontal="true"       />    <TextView        android:layout_width="wrap_content"        android:layout_height="40dp"        android:layout_alignParentRight="true"        android:layout_toRightOf="@+id/cent"        android:gravity="center"        android:text="Timeline"        android:background="@color/white"        android:textColor="@color/md_black_1000"        android:layout_below="@+id/logo"        />    <TextView        android:layout_width="wrap_content"        android:layout_height="2dp"        android:layout_alignParentRight="true"        android:layout_toRightOf="@+id/cent"        android:gravity="center"        android:background="@color/white"        android:textColor="@color/md_black_1000"        android:layout_marginTop="38dp"        android:backgroundTint="@color/gradStart"        android:layout_below="@+id/logo"        />    <ImageView        android:layout_width="2dp"        android:layout_height="40dp"        android:layout_centerHorizontal="true"        android:id="@+id/cent"        android:background="@color/md_grey_200"        android:layout_below="@+id/logo"        />    <TextView        android:layout_width="wrap_content"        android:layout_height="40dp"        android:layout_alignParentLeft="true"        android:layout_toLeftOf="@+id/cent"        android:gravity="center"        android:text="Profile"        android:background="@color/white"        android:textColor="@color/md_black_1000"        android:id="@+id/profile"        android:layout_below="@+id/logo"        /><ScrollView    android:layout_width="match_parent"    android:layout_height="match_parent"    android:layout_below="@+id/cent"    >    <RelativeLayout        android:layout_width="match_parent"        android:layout_height="wrap_content">        <Button            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_marginTop="50dp"            android:textColor="@color/white"            android:background="@drawable/hospbg"            android:text="19/07/2019"            android:id="@+id/tet"            android:layout_marginLeft="10dp"            />        <ImageView            android:layout_width="3dp"            android:layout_height="400dp"            android:background="@color/md_grey_300"            android:layout_below="@+id/tet"            android:layout_marginLeft="40dp"            />        <LinearLayout            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:layout_below="@+id/tet"            android:layout_marginLeft="20dp"            android:id="@+id/ll1"            >            <ImageView                android:layout_width="40dp"                android:layout_height="40dp"                android:src="@drawable/timeline"                android:layout_marginTop="20dp"                />            <TextView                android:layout_marginTop="20dp"                android:layout_width="match_parent"                android:layout_height="80dp"                android:text="Patient is under observation for 6 months"                android:textColor="@color/md_black_1000"                android:background="@color/md_grey_300"                android:layout_marginLeft="20dp"                android:padding="8dp"                />        </LinearLayout>        <Button            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_marginTop="18dp"            android:textColor="@color/white"            android:background="@drawable/hospbg"            android:text="03/08/2019"            android:layout_below="@+id/ll1"            android:layout_marginLeft="10dp"            android:id="@+id/timel"            />        <LinearLayout            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:layout_below="@+id/timel"            android:layout_marginLeft="20dp"            android:id="@+id/ll2"            >            <ImageView                android:layout_width="40dp"                android:layout_height="40dp"                android:src="@drawable/timeline"                android:layout_marginTop="20dp"                />            <TextView                android:layout_marginTop="20dp"                android:layout_width="match_parent"                android:layout_height="80dp"                android:text="Patient is healthy and discharged from hosptital"                android:textColor="@color/md_black_1000"                android:background="@color/md_grey_300"                android:layout_marginLeft="20dp"                android:padding="8dp"                />        </LinearLayout>    </RelativeLayout></ScrollView>    <com.google.android.material.bottomnavigation.BottomNavigationView        android:id="@+id/nav_view"        android:layout_width="match_parent"        android:layout_height="wrap_content"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintLeft_toLeftOf="parent"        app:layout_constraintRight_toRightOf="parent"        app:menu="@menu/bottom_nav_menu"        android:layout_alignParentBottom="true"        android:background="@color/white"        app:labelVisibilityMode="labeled"        app:itemTextColor="@color/md_black_1000"        /></RelativeLayout>